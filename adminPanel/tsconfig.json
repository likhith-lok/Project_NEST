{
  "compilerOptions": {
    "module": "system",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outFile": "bundle.js",
    "lib": ["es2017", "dom"],
    "target": "es2017",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
  },
  "include": ["./*", "adminPanel/adminPanel.ts"],
  "exclude": ["./.venv/"]
}